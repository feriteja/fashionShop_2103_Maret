{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",     
     "experimentalDecorators": true, 
    "baseUrl": "./src",
    "paths":  {
      "reduxAct/*":["./config/redux/actions/*"],
      "myComponents/*":["./components/*"],
      "myScreens/*" :["./screens/*"],
      "myFunction/*":["./utils/functions/*"],
      "myColors/*" :["./assets/theme/*"],
      "myAssets/*":["./assets/*"],
      "myUtils/*":["./utils/*"]
      
      }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}